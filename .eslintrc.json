{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "no-console":"off",
      "global-require":"off",
      "import/no-dynamic-require":"off",
      "no-unused-vars":"off",
      "no-unused-expressions":"off",
      "import/no-unresolved":"off",
      "import/extensions":"off",
      "import/no-extraneous-dependencies":"off"
  }
} 